{
	"name": "jodit-pro",
	"version": "1.3.24",
	"author": "Chupurnov Valeriy <chupurnov@gmail.com>",
	"homepage": "https://xdsoft.net/jodit/pro/",
	"bugs": {
		"url": "https://xdsoft.net/jodit/pro/cab/issues"
	},
	"description": "PRO Version of Jodit Editor",
	"main": "jodit.js",
	"types": "index.d.ts",
	"scripts": {
		"start": "node ./node_modules/jodit/server.js --port=2001",
		"newversion": "yarn && yarn upgrade && npm version patch --no-git-tag-version && npm run changelog && npm run build && npm run newversion:git && node ./prepublish && npm publish ./publish/ && npm run build:react",
		"newversion:git": "git add --all  && git commit -m \"New version $npm_package_version. Read more https://github.com/jodit/jodit-pro/releases/tag/$npm_package_version \" && git tag $npm_package_version && git push --tags origin HEAD:master",
		"clean": "rm -rf ./node_modules/.cache && rm -rf build/*",
		"build:react": "cd ./react && npm version patch --no-git-tag-version  && cd .. && npm run build:react-copy && export NODE_ENV=production && webpack --config=./react/webpack.config.js --mode production && cd react/build && npm publish ./",
		"build:react-copy": "cp ./react/README.md ./react/build/ && cp ./react/LICENSE.md ./react/build/ && cp ./react/package.json ./react/build/ && cp ./node_modules/jodit-react/index.d.ts ./react/build/",
		"build": "npm run clean && npm run build:es5 && npm run build:es2018",
		"build:es5": "node ./build.js --progress --mode production --es es5 --uglify true",
		"build:es2018": "node ./build.js --progress --mode production --es es2018 --uglify true",
		"build:es5:no-uglify": "node ./build.js --progress --mode production --es es5 --uglify false",
		"build:es2018:no-uglify": "node ./build.js --progress --mode production --es es2018 --uglify false",
		"test": "karma start --browsers FirefoxHeadless karma.conf.js",
		"test:debug": "karma start --browsers Chrome karma.conf.js  --single-run false",
		"test:chrome": "karma start --browsers Chrome karma.conf.js",
		"type:build": "rm -rf types/* && tsc --project . --declaration --outDir types --emitDeclarationOnly",
		"changelog": "auto-changelog -p --hide-credit --ignore-commit-pattern tmp",
		"fix": "eslint ./src/ ./test/ --fix && npm run pretty",
		"lint": "eslint ./src/ ./test/ --ignore-pattern fat.ts"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/jodit/jodit-pro.git"
	},
	"resolutions": {
		"jodit": "github:xdan/jodit#master"
	},
	"dependencies": {
		"a-color-picker": "^1.2.1",
		"babel-loader": "^8.2.5",
		"babel-plugin-transform-class-properties": "^6.24.1",
		"jodit": "github:xdan/jodit#master",
		"jodit-react": "^1.3.12"
	},
